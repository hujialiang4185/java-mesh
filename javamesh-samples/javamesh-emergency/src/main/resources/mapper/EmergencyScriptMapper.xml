    <select id="listScript" parameterType="String" resultType="com.huawei.emergency.entity.EmergencyScript">
        SELECT
        s.script_id,
        s.script_name,
        u.user_name as script_user,
        s.submit_info,
        s.script_status,
        s.comment,
        s.update_time,
        s.content,
        s.param,
        s.comment,
        CASE s.script_type
        WHEN '0' THEN 'shell'
        WHEN '1' THEN 'python'
        WHEN '2' THEN 'groovy'
        WHEN '3' THEN 'orchestrate'
        END AS script_type,
        CASE s.have_password
        WHEN '0' THEN '否'
        WHEN '1' THEN '是'
        END AS have_password,
        CASE s.password_mode
        WHEN '0' THEN '本地'
        WHEN '1' THEN '平台'
        END AS password_mode
        FROM
        emergency_script s,nuser u
        <where>
            s.script_user = u.user_id
            <if test="scriptName!=null and scriptName!=''">
                AND s.script_name LIKE CONCAT('%',#{scriptName},'%') ESCAPE '/'
            </if>
            <if test="scriptUser!=null and scriptUser!=''">
                AND s.script_user LIKE CONCAT('%',#{scriptUser},'%') ESCAPE '/'
            </if>
            <if test="auth!='admin' and auth!='approver'">
                AND ((s.is_public = '0' AND s.script_user = #{userName}) OR s.is_public = '1')
            </if>
            <if test="status!='' and status!=null">
                AND s.script_status = '2'
            </if>
        </where>
    </select>

    <select id="getScriptInfo" parameterType="Integer" resultType="com.huawei.emergency.entity.EmergencyScript">
        SELECT script_id,
               script_name,
               CASE
                   is_public
                   WHEN '0' THEN
                       '私有'
                   WHEN '1' THEN
                       '公有'
                   END AS is_public,
